{
	"name": "Hike Mirror",
	"version": "0.1",
	"description": "Mirrors Hike notifications from phone to desktop",
	"manifest_version": 2,
	"permissions": ["gcm", "storage", "notifications", "background", "tabs", "http://*/"],
	"icons": {"128": "ic_launcher.png"},
	"browser_action": {
		"default_title": "Hike Messenger",
		"default_icon": "ic_launcher.png",
		"default_popup": "index.html"
	},

    	"background": {
      		"scripts": ["background.js"]
    	}
  	
}
